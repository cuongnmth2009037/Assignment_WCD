<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LayoutForm</servlet-name>
        <jsp-file>/admin/layout/form.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>LayoutForm</servlet-name>
        <url-pattern>/layout-form</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateCategoryServlet</servlet-name>
        <servlet-class>com.example.assignmentjava.assignmentjava.controller.category.CreateCategoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateCategoryServlet</servlet-name>
        <url-pattern>/category/create</url-pattern>
    </servlet-mapping>
</web-app>